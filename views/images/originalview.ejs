<!DOCTYPE html>
<html lang="en">
  <head>
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css">
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
  </head>
<body>
  <div class="container">

    <div class="page-header text-center">
        <h1><span class="fa fa-cloud"></span> Hunts</h1>
        <a href="/auth/logout" class="btn btn-default btn-sm">Logout</a>
    </div>

    <div class="row">
        <div class="col-12">
            <div class="well">
              
                <% groups.forEach(function(group){ %>
                <img src="data:image/<%= group.original.image.contentType %>;base64, <%= Buffer.from(group.original.image.data).toString('base64') %>"  />
                <form action="/images/<%= group._id %>" method="GET">&emsp; <input type="submit" value="Compare" /></form>
                <% }); %>
              
            </div>
        </div>
    </div>
</div>
<script src="/socket.io/socket.io.js"></script>
<script>
  var socket = io.connect('http://localhost');
  socket.emit('page', 'someone','original');
</script>
</body>
</html>